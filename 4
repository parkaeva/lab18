#include <iostream>

using namespace std;

int main()
{
    int n, m, i, j, max, p, t;
    cin>>n>>m;
    int a[n][m];
    max=-1;
    setlocale(LC_ALL," ");
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<m+1; j++)
        {
            cout<<"a["<<i<<"]["<<j<<"]=";
            cin>>a[i][j];
        }
    }
    for (j=1; j<m+1; j++)
    {
        p=0;
        t=0;
        for (i=1; i<n+1; i++)
        {
            p+=a[i][j];
        }
        p/=n;
        for (i=1; i<n+1; i++)
        {
            if (a[i][j]>p)
            {
                t++;
            }
        }
        cout<<t<<endl;
    }
   
    return 0;
}
