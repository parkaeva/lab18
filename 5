#include <iostream>

using namespace std;

int main()
{
    int n, m, i, j, t;
    cin>>n>>m;
    int a[n][m];
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<m+1; j++)
        {
            cout<<"a["<<i<<"]["<<j<<"]=";
            cin>>a[i][j];
        }
    }
    for (j=1; j<m+1; j++)
    {
        t=0;
        for (i=1; i<n+1; i++)
        {
            t=j;
            if (a[i][j]%2!=0 && t==j)
            {
                t=j;
            } else
            {
                t=0;
            }
        }
        if (t!=0)
        {
            cout<<t;
            break;
        }
    }
   
    return 0;
}
