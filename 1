#include <iostream>

using namespace std;

int main()
{
    int n, i, j, p;
    cin>>n;
    int a[n][n];
    p=(n+1)/2;
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<n+1; j++)
        {
            cout<<"a["<<i<<"]["<<j<<"]=";
            cin>>a[i][j];
        }
    }
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<=1; j++)
        {
            cout<<a[i][j]<<" ";
        }
    }
    cout<<endl;
    for (i=n; i<=n; i++)
    {
        for (j=1; j<=n; j++)
        {
            cout<<a[i][j]<<" ";
        }
    }
    cout<<endl;
    for (i=n; i>=1; i--)
    {
        for (j=n; j<=n; j++)
        {
            cout<<a[i][j]<<" ";
        }
    }
    cout<<endl;
    for (i=1; i==1; i++)
    {
        for (j=n; j>=1; j--)
        {
            cout<<a[i][j]<<" ";
        }
    }
    cout<<endl;
    for (j=2; j<n; j++)
    {
        for (i=2; i<n; i++)
        {
            if (i!=p || j!=p)
            { 
                cout<<a[i][j]<<" ";
            }
        } 
        cout<<endl;
    }
    for (i=1; i<n+1; i++)
    {
        for (j=1; j<n+1; j++)
        {
            if (i==j && i==p && j==p)
            {
                cout<<a[i][j];
            }
        }
    }
    return 0;
}
